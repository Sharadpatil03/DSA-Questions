#include <bits/stdc++.h> 
vector<int> rotateArray(vector<int>& arr, int n) {
    // Write your code here.

    int temp=arr[0];

    for(int i=1; i<n; i++){
        arr[i-1]=arr[i];
    }
    arr[n-1]=temp;

    // extra space : 0(1) 
    // for algorithm space : arr[]
    // TC : O(n)

    return arr;


    
}
